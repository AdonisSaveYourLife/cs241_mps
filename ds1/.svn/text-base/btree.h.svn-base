/** @file btree.h */

#ifndef BTREE_H__
#define BTREE_H__

/**
 * Data structure for each element of the binary tree.
 */
typedef struct _btree_entry_t
{
} btree_entry_t;

/**
 * Binary tree data strucutre.
 */
typedef struct _btree_t
{
	btree_entry_t *root; /**< Points to the root element of the binary tree.
						      Pointer should point to NULL if the binary tree
							  contains to elements (eg: is empty). */
} btree_t;

void btree_init(btree_t* b);
void btree_add(btree_t* b, int value);
void btree_print(btree_t *b);

#endif